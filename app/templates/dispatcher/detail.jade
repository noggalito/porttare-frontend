ion-view(
  view-title="{{:: categoryVm.category.titulo}}"
)
  ion-nav-title
    .alt-logo
  ion-content.dispatcher-page
    .row
      .col.col-67.detailTitle {{dispatchersVm.dispatcher.user.name}}
      .col.col-33.detailActions
        a.edit.light-blue {{::("globals.edit"|translate)}}
        a.delete.light-red {{::("globals.delete"|translate)}}
    .row
      .col.detailSubTitle {{::("dispatcher.dataDispatcher"|translate)}}
    .containerDispatcher
      .row.line-buttom
        .col.col-50.key {{::("dispatcher.dispatcherId"|translate)}}
        .col.col-50.value {{dispatchersVm.dispatcher.id}}
      .row.line-buttom
        .col.col-50.key {{::("dispatcher.dispatcherEmail"|translate)}}
        .col.col-50.value {{dispatchersVm.dispatcher.email}}
      .row
        .col.col-50.key {{::("dispatcher.office"|translate)}}
        .col.col-50.value {{dispatchersVm.dispatcher.provider_office_id}}
